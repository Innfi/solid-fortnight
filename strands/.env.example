# Environment variables for Strands A2A examples

# OpenAI API Key
OPENAI_API_KEY=sk-your-openai-api-key-here

# Remote Agent URLs
REMOTE_AGENT_URL=http://localhost:8000

# Optional: Multiple agent URLs for distributed setup
WEATHER_AGENT_URL=http://localhost:8001
DATA_AGENT_URL=http://localhost:8002
CALCULATOR_AGENT_URL=http://localhost:8003

# Server Configuration
A2A_SERVER_HOST=0.0.0.0
A2A_SERVER_PORT=8000

# Timeout and Retry Configuration
A2A_TIMEOUT=30.0
A2A_RETRY_COUNT=3
A2A_RETRY_DELAY=1.0

# Logging
LOG_LEVEL=INFO

# Security (for production)
A2A_API_KEY=your-api-key-here
ENABLE_AUTH=false